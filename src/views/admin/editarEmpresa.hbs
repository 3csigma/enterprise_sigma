<link rel="stylesheet" href="../css/editarEmpresa.css">
<style>
    .chosen-container-single .chosen-single span {
        font-size: 14px;
    }
    .mostrar{
        display: none;
    }
    .table td{
        padding: 12px 15px;
    }
</style>
<!--**********************************
        Content body start
***********************************-->
<div class="content-body">
    <!-- row -->
    <div class="container-fluid">

        {{!-- Header Card --}}
        <div class="col-lg-12">
            <div class="profile card card-body px-3 pt-3 pb-0">
                <div class="profile-head">
                    <div class="profile-info">
                        <div class="profile-photo">
                            {{#if datos.foto}}
                            <img src="{{datos.foto}}" class="img-fluid rounded-circle" style="height: 100px;object-fit: cover;" alt="Foto_Perfil_Empresa">
                            {{else}}
                            <img src="../img/profile_default/user.jpg" class="img-fluid rounded-circle" style="height: 100px;object-fit: cover;" alt="Foto_Perfil_Empresa">
                            {{/if}}
                        </div>
                        <div class="profile-details">
                            <div class="profile-name px-3 pt-2">
                                <h4 class="text-primary mb-0">{{datos.nombre_completo}}</h4>
                                <p id="nombreEmpresa">{{datos.nombre_empresa}}</p>
                            </div>
                            <div class="profile px-2 pt-2">
                                <h4 class="text-muted mb-0">{{#if
                                    empresa.telefono}}{{empresa.telefono}}{{else}}N/A{{/if}}</h4>
                                <p>Teléfono</p>
                            </div>
                            <div class="profile px-2 pt-2">
                                <h4 class="text-muted mb-0">
                                    <i class="fa-solid fa-eye" id="icono-verDiv" style="display: none; cursor: pointer;" onclick="mostrar_DivIzq()"></i>
                                    <i class="fa-solid fa-eye-slash" id="icono-noVerDiv" style="cursor: pointer;" onclick="ocultar_DivIzq()"></i>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>

        {{!-- ./Fin Header Card --}}

        <div class="row"> 

            {{!-- COLUNMA IZQUIERDA RESUMEN --}}
            <div id="col_izq" class="col-xl-4">
                <div class="card">
                    <div class="card-body">
                        <div class="profile-statistics">
                            <div class="text-center">
                                <div class="row">
                                    <div class="col">
                                        <h3 class="m-b-0">{{#if
                                            empresa.cantidad_socios}}{{empresa.cantidad_socios}}{{else}}N/A{{/if}}</h3>
                                        <span>Cantidad de Socios</span>
                                    </div>
                                    <div class="col">
                                        <h3 class="m-b-0">{{#if empresa.tiempo_experiencia}}
                                            {{empresa.tiempo_experiencia}}{{else}}N/A{{/if}} </h3><span>Años de
                                            experiencia</span>
                                    </div>
                                    <div class="col">
                                        <h3 class="m-b-0">{{#if empresa.num_empleados}}
                                            {{empresa.num_empleados}}{{else}}N/A{{/if}} </h3><span>Número de
                                            Empleados</span>
                                    </div>
                                </div>

                                <div class="mt-4">
                                    <input type="hidden" id="idEmpresa" value="{{datos.idEmpresa}}">
                                    <input type="hidden" id="idConsultor" value="{{datos.idConsultor }}">
                                    <input type="hidden" id="datosUsuario" value="{{datosUsuario}}">
                                    {{#if adminDash}}
                                    {{!-- Fila para Selectores Asginar consultor & Estado de la cuenta --}}
                                    <div class="row">
                                        <div class="col-12">
                                            <label>Estado de la empresa</label>
                                            <select id="estadoAdm" class="mr-sm-2 default-select text-center">
                                                {{#comparar datos.estadoAdm '==' '1'}}
                                                <option value="1" selected>Activa</option>
                                                <option value="0">Bloqueada</option>
                                                {{/comparar}}
                                                {{#comparar datos.estadoAdm '!=' '1'}}
                                                <option value="0" selected>Bloqueada</option>
                                                <option value="1">Activa</option>
                                                {{/comparar}}
                                            </select>
                                        </div>
                                    </div>
                                    
                                    <div class="row mt-3">
                                        
                                        <div class="col-12">
                                            <label>Consultor <span class="text-red" id="txtNulo1" style="display: none;">*Obligatorio*</span></label>
                                            <select id="id_consultor" onchange="validar_nivelConsultor()" class="form-control sltBuscar chosen-container chosen-container-single">
                                                <option value=""></option>
                                                {{#each consultores}}
                                                <option id="{{nivel}}" {{#comparar id_consultores '==' idCon}} selected {{/comparar}} value="{{id_consultores}}">
                                                    {{nombres}} {{apellidos}} - Nivel {{nivel}}</option>
                                                {{/each}}
                                            </select>
                                        </div>

                                        <div class="col-12 mt-3">
                                            <label>Etapa <span class="text-red" id="txtNulo2" style="display: none;">*Obligatorio*</span></label>
                                            <select id="etapaConsultor" class="selectpicker" onchange="validar_nivelEtapa()">
                                                <option value="0" selected>Selecciona una opción</option>
                                                <option value="Diagnóstico">Diagnóstico</option>
                                                <option value="Análisis">Análisis</option>
                                                <option value="Plan_Empresarial">Plan Empresarial</option>
                                                <option value="Plan_Estratégico">Plan Estratégico</option>
                                            </select>
                                        </div>

                                        <div class="col-12 mt-3" id="divSede" style="display:none">
                                            <label>Sede</label>
                                            <select id="sede" class="selectpicker">
                                                <option value="1">Online</option>
                                                <option value="2">Miami</option>
                                                <option value="3">Resto de USA</option>
                                            </select>
                                        </div>

                                        <div class="col-12 pt-3">
                                            <button type="button" onclick="addConsulEtapa()" class="btn color_primary text-white mt-4 w-100" style="border-color: transparent">
                                                Asignar Consultor <i class="fa fa-check"></i></button>
                                        </div>

                                        <div class="col-12" id="divTablaConsul" style="display: {{divConsultores}}">
                                            <table id="tblConsulAsignados" class="table text-center">
                                                <thead>
                                                    <tr>
                                                        <th>Etapa</th>
                                                        <th>Consultor</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    {{#each consultores_asignados}}
                                                    <tr id="{{idFila}}">
                                                        <td>{{etapa}}</td>
                                                        <td >{{consultor}}</td>
                                                    </tr>
                                                    {{/each}}
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                    {{/if}}

                                    {{!-- Boton actualizar empresa --}}
                                    <div class="row">
                                        <input type="hidden" value="{{datos.code}}" name="codigo" id="codigoEmpresa">
                                        {{#if adminDash}}
                                        <div class="col-12">
                                            <div class="form-group">
                                                <button type="button" class="btn color_primary text-white w-100"
                                                    onclick="guardarConsulEtapas()"
                                                    style="margin-top: 1em; padding: 10px 15px;">
                                                    <p class="font-w500 m-auto">Guardar cambios</p>
                                                </button>
                                            </div>
                                        </div>
                                        {{/if}}

                                    </div>

                                    <div class="row mt-4">
                                        <div class="col">
                                            <div class="form-group" style="text-align: left !important">
                                                <label style="font-size:22px;color:#000;font-weight:600;margin-bottom: 20px;">Descripción del negocio</label>
