                                                    style="height: 200px; min-height: 200px; max-height: 200px; width: 100%; text-align: justify; background: transparent !important; border: none"
                                                    class="form-control barra-scroll overflow-y-auto"
                                                    disabled>{{#if empresa.descripcion}}{{empresa.descripcion}}{{else}}N/A{{/if}}</textarea>
                                            </div>
                                        </div>
                                    </div>

