                                                            style="background: #656c73;margin: 0 auto;border-color: #656c73;">
                                                                <i class="fa fa-pencil"></i></button>
                                                        {{/if}}
                                                        </div>
                                                    </div>
                                                </td>
                                                {{/unless}}
                                                <td class="precios_celdas">{{propuesta.precio_total}}</td>
                                                {{#if propuesta.fechaCancelacion}}
                                                <td>{{propuesta.fechaCancelacion}}</td>
                                                {{/if}}
                                                <td>
                                                    <a href="{{propuesta.link_propuesta}}" target="_blank"
                                                        class="btn color_gradient shadow btn-xs sharp text-white"
                                                        style="margin: 0 auto">
                                                        <i class="fa fa-search"></i>
                                                    </a>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                {{#if propuesta.pagada}}
                {{!-- INFORME GENERAL - PLAN ESTRATÉGICO --}}
